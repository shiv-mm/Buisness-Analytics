\n    },\n    {\n      \"column\": \"remaining\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 150.69948020259835,\n        \"min\": 532.0,\n        \"max\": 802.0,\n        \"num_unique_values\": 3,\n        \"samples\": [\n          532.0,\n          783.0,\n          802.0\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"}},"metadata":{}}]},{"cell_type":"code","source":["def run_scenario(unmet_penalty=10.0, inventory_multiplier=1.0):\n","    inv2 = inventory.copy()\n","    inv2[\"on_hand\"] = (inv2[\"on_hand\"] * inventory_multiplier).astype(int)\n","\n","    W = inv2[\"warehouse_id\"].unique().tolist()\n","    S = demand_h[\"sku_id\"].unique().tolist()\n","    P = demand_h[\"pincode\"].unique().tolist()\n","\n","    inv_ws = {(row.warehouse_id, row.sku_id): int(row.on_hand) for row in inv2.itertuples()}\n","    dem_sp = {(row.sku_id, row.pincode): int(row.demand) for row in demand_h.itertuples()}\n","    cost_wp = {(row.warehouse_id, row.pincode): float(row.eff_cost) for row in dist_df.assign(eff_cost=lambda d: d[\"ship_cost\"]+0.8*d[\"sla_breach\"]).itertuples()}\n","\n","    prob = pulp.LpProblem(\"Scenario\", pulp.LpMinimize)\n","    x = pulp.LpVariable.dicts(\"alloc\", ((w,s,p) for w in W for s in S for p in P), lowBound=0, cat=\"Continuous\")\n","    unmet = pulp.LpVariable.dicts(\"unmet\", ((s,p) for s in S for p in P), lowBound=0, cat=\"Continuous\")\n","\n","    prob += pulp.lpSum([cost_wp[(w,p)] * x[(w,s,p)] for w in W for s in S for p in P if (w,p) in cost_wp]) \\\n","            + unmet_penalty * pulp.lpSum([unmet[(s,p)] for s in S for p in P])\n","\n","    for w in W:\n","        for s in S:\n","            cap = inv_ws.get((w,s), 0)\n","            if cap > 0:\n","                prob += pulp.lpSum([x[(w,s,p)] for p in P]) <= cap\n","\n","    for s in S:\n","        for p in P:\n","            dem = dem_sp.get((s,p), 0)\n","            if dem > 0:\n","                prob += pulp.lpSum([x[(w,s,p)] for w in W]) + unmet[(s,p)] >= dem\n","\n","    _ = prob.solve(pulp.PULP_CBC_CMD(msg=False))\n","\n","    total_demand = demand_h[\"demand\"].sum()\n","    alloc_units = sum([x[(w,s,p)].value() or 0 for w in W for s in S for p in P])\n","    unmet_units = sum([unmet[(s,p)].value() or 0 for s in S for p in P])\n","    return {\n","        \"unmet_penalty\": unmet_penalty,\n","        \"inventory_multiplier\": inventory_multiplier,\n","        \"fulfillment_rate\": 100.0 * alloc_units / total_demand,\n","        \"unmet_units\": unmet_units\n","    }\n","\n","print(run_scenario(unmet_penalty=10.0, inventory_multiplier=1.0))\n","print(run_scenario(unmet_penalty=20.0, inventory_multiplier=1.0))\n","print(run_scenario(unmet_penalty=10.0, inventory_multiplier=1.2))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"EjHpdada1j7w","executionInfo":{"status":"ok","timestamp":1756538329609,"user_tz":-330,"elapsed":1025,"user":{"displayName":"","userId":""}},"outputId":"8ec3d761-6d6a-4d46-f4d9-48360b90e350"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["{'unmet_penalty': 10.0, 'inventory_multiplier': 1.0, 'fulfillment_rate': np.float64(18.26654913804315), 'unmet_units': 113124.0}\n","{'unmet_penalty': 20.0, 'inventory_multiplier': 1.0, 'fulfillment_rate': np.float64(20.32354088695577), 'unmet_units': 110277.0}\n","{'unmet_penalty': 10.0, 'inventory_multiplier': 1.2, 'fulfillment_rate': np.float64(20.970911665679232), 'unmet_units': 109381.0}\n"]}]}],"metadata":{"colab":{"provenance":[{"file_id":"/v2/external/notebooks/intro.ipynb","timestamp":1756539426737}]},"kernelspec":{"display_name":"Python 3","name":"python3"}},"nbformat":4,"nbformat_minor":0}